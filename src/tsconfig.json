{
    "compilerOptions": {
      "types": ["vite/client"],
      "target": "ES3",
      "strict": true,
      "noImplicitThis": true,
       "noImplicitAny": true,
       "strictNullChecks": true,
       "jsx": "preserve",
       "moduleResolution": "node"
    },
    "include": ["src/**/*.ts","src/**/*.d.ts","src/**/*.vue"]
  }